<div class="container">
{{#if questionSend}}
  <ul class="cards">
    {{#each data.cards}}
      <li>
        {{url.original}}
      </li>
    {{/each}}
  </ul>
{{else}}

  <div class="row">
  <div class="col col-md-6 col-md-offset-3">
    <h1 class="stylist-question-title">O que vocÃª deseja?</h1>

    <form action="/stylist" method="post">
      <div class="form-group">
        <input class="form-control input-lg" name="question" placeholder="Nos conte :)" value="{{questionSend}}" />
      </div>
      <div class="pull-right">
        <button class="btn btn-success btn-lg">Pesquisar</button>
      </div>
    </form>

  </div>
{{/if}}
</div>